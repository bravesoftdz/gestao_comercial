  object pQuery: TFDQuery
    Connection = FrModulo.DBCon_Local
    SQL.Strings = (
      'SELECT'
      '   F_CUPOM_NUMERO,'
      '   CAST(F_CUPOM_DATA AS DATE) AS F_CUPOM_DATA,'
      '   F_XML,'
      '   F_XML_CHAVE,'
      '   F_CUPOM_STATUS,'
      '   F_VLR_TOTAL AS F_VLR_CUPOM,'
      ''
      '   IIF(F_CUPOM_STATUS = 1, F_VLR_TOTAL, 0)  AS F_VLR_CUPOM_A,'
      '   IIF(F_CUPOM_STATUS = 2, F_VLR_TOTAL, 0)  AS F_VLR_CUPOM_C,'
      '   IIF(F_CUPOM_STATUS = 2, '#39'Cancelado'#39', '#39#39') AS F_CANCELADO,'
      ''
      
        '   (SELECT F_DESCRICAO FROM C000017 WHERE ID = C000050.ID_CAIXA)' +
        ' AS F_CAIXA_DESCRICAO'
      ''
      'FROM'
      '   C000050'
      'WHERE'
      '   CAST(F_CUPOM_DATA AS DATE) BETWEEN :ADATA AND :FDATA'
      'AND'
      '   F_CUPOM_DIRETIVA = 1')
    Left = 152
    Top = 136
    ParamData = <
      item
        Name = 'ADATA'
        DataType = ftString
        ParamType = ptInput
        Value = '01/01/2018'
      end
      item
