    SQL.Strings = (
      'SELECT'
      
        '   ID, (LPAD(FUN_CODIGO,2,0) || '#39' - '#39' || FUN_APELIDO) AS DESCRIC' +
        'AO'
      'FROM'
      '   C000301'
      'WHERE'
      '   ID_EMPRESA   = :ID_EMPRESA'
      'AND'
      '   FUN_VENDEDOR = 1'
