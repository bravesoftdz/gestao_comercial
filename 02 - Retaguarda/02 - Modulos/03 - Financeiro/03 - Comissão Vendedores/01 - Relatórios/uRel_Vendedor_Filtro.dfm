  object Query: TFDQuery
    Connection = FrModuloRet.DBConexao
    SQL.Strings = (
      'SELECT'
      
        '   (SELECT LPAD(FUN_CODIGO,02) || '#39' - '#39' || FUN_APELIDO AS FUN_AP' +
        'ELIDO FROM C000301 WHERE ID = T2.ID_VENDEDOR) FUN_APELIDO,'
      '   T2.*'
      'FROM'
      '   C000030 AS T2'
      'WHERE'
      '   T2.ID_EMPRESA   = :ID_EMPRESA'
      'AND'
      '   T2.F_CUP_STATUS = 1'
      'AND'
      '   CAST(T2.F_CUP_DATA AS DATE) BETWEEN :ADATA AND :FDATA')
    Left = 6
    Top = 128
    ParamData = <
      item
        Name = 'ID_EMPRESA'
        DataType = ftInteger
        ParamType = ptInput
        Value = 1
      end
      item
        Name = 'ADATA'
        DataType = ftDate
        ParamType = ptInput
        Value = 43466d
      end
      item
        Name = 'FDATA'
        DataType = ftDate
        ParamType = ptInput
        Value = 43831d
      end>
